local CrayolaHub = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()

local Window = CrayolaHub.CreateLib('Crayola Hub', 'Serpent')

local Player = game.Players.LocalPlayer
local Character = Player.Character
local Backpack = Player:FindFirstChild("Backpack")
local Humanoid = Character:WaitForChild('Humanoid')
local Root = Character:WaitForChild('HumanoidRootPart')

local TweenService = game:GetService('TweenService')

--// Variables --\\

local Webhook = nil
getgenv().AutoPushup = true


--// Actual Tabs --\\

local MainTab = Window:NewTab('Main Tab')
local AutoFarmTab = Window:NewTab('Auto Farm')
local GamepassTab = Window:NewTab('Gamepasses')
local MiscTab = Window:NewTab('Misc')

--// Sections Inside The Tabs --\\

local MainSection = MainTab:NewSection('Main Section')
local AutoFarmSection = AutoFarmTab:NewSection('Auto Farm')
local GamepassSection = GamepassTab:NewSection('Gamepass')
local MiscSection = MiscTab:NewSection('Misc')


-- [[ Buttons ]] --

MiscSection:NewButton('Kill Me', 'Kills your player locally.', function()
  print( 'You have been killed.')
  Humanoid.Health = -1
end)

MainSection:NewButton("Auto Pushup On", "Test", function()
  getgenv().AutoPushup = true
    while getgenv().AutoPushup == true do wait()
workspace.Map.Chunk23.Push_Ups_Mat.Initiated:FireServer()
    for i=1,3 do
      pcall(function()
local VirtualInputManager = game:GetService('VirtualInputManager')


for i,Front in pairs(Player.PlayerGui.ExcessGuis["Push_Up_Gui"].Holder:GetChildren()) do
        if Front.Name == "Front" then
        VirtualInputManager:SendKeyEvent(true, Front.Text, false, game)
        wait()
        VirtualInputManager:SendKeyEvent(false, Front.Text, false, game)
    end
end

for i,Back in pairs(Player.PlayerGui.ExcessGuis["Push_Up_Gui"].Holder:GetChildren()) do
        if Back.Name == "Back" then
        VirtualInputManager:SendKeyEvent(true, Back.Text, false, game)
        wait()
        VirtualInputManager:SendKeyEvent(false, Back.Text, false, game)
                end
            end
        end)
    end
    end
end)

MainSection:NewButton("Auto Pushup Off", "Test", function()
  getgenv().AutoPushup = false
end)

-- [[ Toggles ]] --



-- [[ Text Boxes ]] --

AutoFarmSection:NewTextBox("Drop Webhook", "Sends The Drop You've Recieved To The Inputted Webhook.", function(webhooktext)
    print(webhooktext)
    Webhook = webhooktext
end)
